# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=security_portal

## Server connection string (must be fully expanded, no placeholders)
DATABASE_URL=postgres://postgres:postgres@db:5432/security_portal

# Optional Redis (enable with COMPOSE_PROFILES=redis)
# REDIS_URL=redis://redis:6379/0

# PostgREST (optional JWT if you plan to secure queries)
# POSTGREST_JWT_SECRET=dev-placeholder

# Scanner image to enable real Shai-Hulud execution from the server
SCANNER_IMAGE=auditgh-scanner:latest
CODEQL_RAM_MIB=16384
CODEQL_THREADS=1

# OSS Scanner defaults (optional)
SYFT_FORMAT=cyclonedx-json
GRYPE_SCAN_MODE=both

# Seeding concurrency (optional; used by setup.sh and seeders)
OSS_MAX_WORKERS=3
LINECOUNT_MAX_WORKERS=3
ENGAGEMENT_MAX_WORKERS=3

# Periodic seeders (compose --profile seed)
SEED_INTERVAL=3600
SEED_LANGLOC_INTERVAL=86400